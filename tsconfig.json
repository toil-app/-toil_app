{
  "extends": "@react-native/typescript-config",
  "compilerOptions": {
    "types": [
      "jest"
    ],
    "jsx": "react",
    "noEmit": true,
    "allowJs": true,
    "baseUrl": "./",
    "paths": {
      "@components/*": [
        "src/component/*"
      ],
      "@core/*": [
        "src/core/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@utils/*": [
        "src/core/util/*"
      ],
      "@module/*": [
        "src/module/*"
      ],
      "@module": [
        "src/module"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.d.ts"
  ],
  "exclude": [
    "**/node_modules",
    "**/Pods"
  ]
}